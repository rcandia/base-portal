{
  "name": "channel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 4000 --open",
    "build": "vite build",
    "preview": "concurrently 'vite build --watch' 'vite preview --port 4000'",
    "commit": "npx git-cz",
    "lint": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,vue}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/node": "^17.0.36",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "concurrently": "^7.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.3",
    "prettier": "^2.6.2",
    "single-spa": "^5.9.3",
    "single-spa-layout": "2.0.1",
    "single-spa-vue": "^2.5.1",
    "typescript": "^4.7.2",
    "unocss": "^0.39.3",
    "vite": "^2.9.9",
    "vue": "^3.2.37"
  },
  "dependencies": {},
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
